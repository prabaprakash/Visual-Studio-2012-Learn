<Window x:Class="Smart_Thread.Views.Home"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Home" Height="300" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="43*"/>
            <RowDefinition Height="21*"/>
            <RowDefinition Height="23*"/>
            <RowDefinition Height="24*"/>
            <RowDefinition Height="21*"/>
            <RowDefinition Height="26*"/>
            <RowDefinition Height="37*"/>
            <RowDefinition Height="27*"/>
            <RowDefinition Height="47*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="25*"/>
            <ColumnDefinition Width="103*"/>
            <ColumnDefinition Width="164*"/>
        </Grid.ColumnDefinitions>
        <TextBox x:Name="proxy" Grid.Row="1"  Grid.Column="1" />
        <Button x:Name="startThread" Click="StartThread_OnClick" Grid.Column="1" Grid.Row="7" Height="27" VerticalAlignment="Top"/>
        <TextBox x:Name="min" Grid.Column="1" Grid.Row="3"/>
        <TextBox x:Name="max" Grid.Column="1" Grid.Row="5"/>
        <ListBox Name="ls1" Grid.Column="2" HorizontalAlignment="Left" Height="179" Grid.RowSpan="7" VerticalAlignment="Top" Width="164" Grid.Row="1">
            <Button Content="Button" Width="75" Click="Button_Click"/>
        </ListBox>

    </Grid>
</Window>
